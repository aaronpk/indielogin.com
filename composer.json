{
    "require": {
        "guzzlehttp/guzzle": "^7.9",
        "indieauth/client": "~1.1",
        "indieweb/link-rel-parser": "^0.1.3",
        "j4mie/idiorm": "^1.5",
        "laminas/laminas-diactoros": "^3.5",
        "laminas/laminas-httphandlerrunner": "^2.11",
        "league/container": "^4.2",
        "league/plates": "^3.3",
        "league/route": "^5.1",
        "mailgun/mailgun-php": "^4.3",
        "masterminds/html5": "^2.7",
        "mf2/mf2": "^0.4.6",
        "michelf/php-markdown": "^2.0",
        "monolog/monolog": "^3.8",
        "p3k/http": "^0.1.7",
        "p3k/utils": "^1.2",
        "predis/predis": "^2",
        "vlucas/phpdotenv": "^4.1"
    },
    "autoload": {
      "psr-4": {
        "App\\": "app/"
      },
      "files": [
        "lib/helpers.php"
      ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "scripts": {
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "php -S localhost:8080 -t public"
        ]
    }
}
