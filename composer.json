        "j4mie/idiorm": "^1.5",
        "laminas/laminas-diactoros": "^3.0",
        "laminas/laminas-httphandlerrunner": "^2.10",
        "league/plates": "^3.3",
        "league/route": "^3.0",
        "mailgun/mailgun-php": "^4.0",
        "masterminds/html5": "^2.7",
        "mf2/mf2": "^0.4.6",
        "michelf/php-markdown": "^1.8",
